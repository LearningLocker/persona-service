{
  "extends": [
    "./node_modules/jscommons/tslint.json",
    "tslint-immutable"
  ],
  "rules": {
    "readonly-keyword": true,
    "readonly-array": false, // We'd like this in future.
    "no-let": true,
    "no-this": true,
    "no-class": true,
    "no-mixed-interface": true,
    "no-expression-statement": false,
    "match-default-export-name": false // We'd like this in future.
  }
}
